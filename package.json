{
  "name": "hooka",
  "version": "0.1.0",
  "description": "A webhook server with zero coding",
  "bin": "bin/index.js",
  "main": "lib/server/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src/ -d lib/ --source-maps",
    "build:watch": "npm run build -- --watch",
    "precommit": "npm run test:all",
    "prepublish": "npm run build",
    "test:all": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint .",
    "test:unit": "jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "test:watch": "nodemon --exec npm run test:unit"
  },
  "keywords": [
    "webhook",
    "server",
    "webhooks",
    "hook",
    "hooks"
  ],
  "author": "Daniel Stefanovic",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.3.26",
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-plugin-rewire": "1.0.0-beta-2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "babel-register": "6.3.13",
    "babel-template": "6.3.13",
    "babel-types": "6.4.1",
    "eslint": "1.10.3",
    "eslint-config-infektweb": "1.6.0",
    "eslint-plugin-bdd": "1.1.1",
    "eslint-plugin-react": "3.14.0",
    "jasmine": "2.4.1",
    "nodemon": "1.8.1",
    "rimraf": "2.5.0",
    "source-map-support": "0.4.0"
  },
  "dependencies": {
    "express": "4.13.3",
    "yargs": "3.31.0"
  }
}
